  "scripts": {
    "start": "expo start",
    "start:dev": "expo start --dev-client",
    "start:clear": "expo start --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
